    "@types/express": "^4.17.13",
    "@types/jasmine": "~3.10.3",
    "@types/node": "^20.8.7",
    "jasmine-core": "~4.0.0",
    "jasmine-marbles": "~0.8.4",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.7.0",
    "purgecss-webpack-plugin": "^4.1.3",
    "typescript": "~4.9.5"
