{
  "name": "jankuri.me",
  "version": "0.0.0",
  "description": "Jan Kuri's personal homepage",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build-dev": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run jankuri-me:serve-ssr",
    "serve:ssr": "node dist/server/main.js",
    "build:ssr": "ng build --configuration production && ng run jankuri-me:server:production",
    "prerender": "ng run jankuri-me:prerender",
    "vercel-build": "yarn run build:ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.10",
    "@angular/common": "^16.2.10",
    "@angular/compiler": "^16.2.10",
    "@angular/core": "^16.2.10",
    "@angular/forms": "^16.2.10",
    "@angular/platform-browser": "^16.2.10",
    "@angular/platform-browser-dynamic": "^16.2.10",
    "@angular/platform-server": "^16.2.10",
    "@angular/router": "^16.2.10",
    "@fortawesome/fontawesome-free": "^6.0.0",
    "@nguniversal/express-engine": "^16.2.0",
    "bulma": "^0.9.3",
    "d3": "^7.3.0",
    "express": "^4.17.3",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.1",
    "zone.js": "~0.13.3"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^16.0.1",
